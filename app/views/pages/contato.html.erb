<div class="card">
  <div class="card-header">
    <h1>Fale conosco</h1>
  </div>
  <div class="card-body">
    <form>
      <div class="form-group">
        <label for="formGroupExampleInput">Nome</label>
        <input type="text" class="form-control" id="formGroupExampleInput" placeholder="nome completo">
      </div>
      <div class="form-group">
        <label for="exampleFormControlInput1">Email</label>
        <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="nome@exemplo.com">
      </div>
      <div class="form-group">
        <label for="exampleFormControlTextarea1">Deixe sua mensagem</label>
        <textarea class="form-control" id="exampleFormControlTextarea1" placeholder="mensagem" rows="3"></textarea>
      </div>
      </form>
  </div>
  <div class="card-footer">
    <button type="button" class="btn btn-primary">Enviar</button>
  </div>
</div>
